# WooCommerce API Configuration
WOOCOMMERCE_URL=https://mayasquare.com
WOOCOMMERCE_CONSUMER_KEY=ck_your_consumer_key_here
WOOCOMMERCE_CONSUMER_SECRET=cs_your_consumer_secret_here

# Data Extraction Settings
# How many orders to fetch per API call (max: 100)
ORDERS_PER_PAGE=100

# Date range for extraction (ISO format)
# Leave empty to fetch all orders
START_DATE=
END_DATE=

# Store Identification Method
# Options: city, metadata, billing
# city: Map shipping city to stores (default)
# metadata: Use custom order metadata field
# billing: Use billing address state/region
STORE_IDENTIFICATION_METHOD=city

# Custom Metadata Field Name (only used if STORE_IDENTIFICATION_METHOD=metadata)
STORE_METADATA_FIELD=_store_id

# Store Mapping Configuration (JSON array)
# Each entry maps a city pattern to a store name
# Wildcards: Use * at the end for partial matches
STORE_MAPPING=[
  {"city_pattern": "Paris*", "store": "Paris Store"},
  {"city_pattern": "Lyon*", "store": "Lyon Store"},
  {"city_pattern": "Marseille*", "store": "Marseille Store"},
  {"city_pattern": "Bordeaux*", "store": "Bordeaux Store"},
  {"city_pattern": "Lille*", "store": "Lille Store"},
  {"city_pattern": "Nice*", "store": "Nice Store"},
  {"city_pattern": "Nantes*", "store": "Nantes Store"},
  {"city_pattern": "Strasbourg*", "store": "Strasbourg Store"}
]

# Order Status to Include (comma-separated)
# Common values: completed, processing, cancelled, refunded
ORDER_STATUS=completed

# Currency (for formatting)
CURRENCY=EUR

# Logging Level
# Options: debug, info, warn, error
LOG_LEVEL=info
